    "import torch.nn as nn\n",
    "class CNN(nn.Module):\n",
    "    def __init__(self):\n",
    "        super(CNN, self).__init__()\n",
    "        self.conv1 = nn.Sequential(         \n",
    "#           1x28x28 -> 16x24x24\n",
    "            nn.Conv2d(\n",
    "                in_channels=1,              \n",
    "                out_channels=16,            \n",
    "                kernel_size=5,              \n",
    "                stride=1,                   \n",
    "                padding=0,                  \n",
    "            ),                              \n",
    "            nn.ReLU(),\n",
    "#           16x24x24 -> 16x12x12\n",
    "            nn.MaxPool2d(\n",
    "                kernel_size=2,\n",
    "                stride=2 \n",
    "            ),    \n",
    "        )\n",
    "        self.conv2 = nn.Sequential(         \n",
    "#           16x12x12 -> 32x10x10  \n",
    "            nn.Conv2d(\n",
    "                in_channels = 16, \n",
    "                out_channels = 32, \n",
    "                kernel_size = 3, \n",
    "                stride = 1, \n",
    "                padding = 0,\n",
    "            ),     \n",
    "            nn.ReLU(),\n",
    "#           32x10x10 -> 32x5x5\n",
    "            nn.MaxPool2d(2, 2),                \n",
    "        )\n",
    "        # fully connected layer, output 10 classes\n",
    "        self.out = nn.Linear(32 * 5 * 5, 10)\n",
    "    \n",
    "    def forward(self, x):\n",
    "        x = self.conv1(x)\n",
    "        x = self.conv2(x)\n",
    "        # flatten the output of conv2 to (batch_size, 32 * 5 * 5)\n",
    "        x = x.view(x.size(0), -1)       \n",
    "        output = self.out(x)\n",
    "        return output, x    # return x for visualization"
   ]
